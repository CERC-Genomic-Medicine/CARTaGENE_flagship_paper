params {
//Files
vcf="/scratch/vct/CARTaGENE_v1.1/Test/out/S5/VCF/CAG_Harmonized.vcf.gz"
ref="/lustre06/project/6061810/shared/TOPMed/Freeze8/ALL.BRAVO_TOPMed_Freeze_8.vcf.gz"
OutDir="out/topmed"


//Parameters
Batch_length=25000
name='CaG'
Threshold=0.2

}

process {
 withLabel: 'Testing' {
 cpus = 1
  time = "24h"
  memory = "12GB"
    }
  withLabel: 'Chromosome_split' {
 cpus = 1
  time = "24h"
  memory = "12GB"
    }
  executor = "slurm"
  	// can add --account=[] to cluster Options
  clusterOptions = "--account=rrg-vmooser"
  cpus = 1
  time = "12h"
  memory = "10GB"
}
executor {
        $slurm {
              queueSize = 500
              jobName = { "TOPMed_precheck" }
        }
        $local {
                cpus = 1
        }
}
